{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
//    "jest/global": true
  },
  "rules": {
"semi": "off",
    "arrow-parens": "off",
    "linebreak-style": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "no-trailing-spaces": "off",
    "operator-linebreak": "off"
  },
//  "plugin": ["jest"],
  "extends": ["eslint:recommended", "google"]
}